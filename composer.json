{
    "name": "julienlinard/php-skeleton",
    "type": "project",
    "description": "Skeleton minimal utilisant mes libraries PHP (router, core, auth, doctrine).",
    "keywords": [
        "skeleton",
        "php",
        "php8",
        "framework",
        "mvc",
        "starter",
        "template"
    ],
    "homepage": "https://github.com/julien-lin/php-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Linard",
            "email": "julien.linard.dev@gmail.com",
            "homepage": "https://github.com/julien-lin"
        }
    ],
    "support": {
        "issues": "https://github.com/julien-lin/php-skeleton/issues",
        "source": "https://github.com/julien-lin/php-skeleton"
    },
    "require": {
        "php": "^8.1",
        "julienlinard/core-php": "^1.0",
        "julienlinard/php-router": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Julien\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Julien\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "Julien\\Installer::postInstall"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.1"
}

